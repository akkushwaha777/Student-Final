{
    "compilerOptions": {
        "target": "ES6",
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "module": "ESNext",
        "skipLibCheck": true,
        "moduleResolution": "bundler",
        "allowImportingTsExtensions": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "checkJs": false
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ]
}